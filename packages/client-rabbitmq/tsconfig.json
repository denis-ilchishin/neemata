{
  "compilerOptions": {
    "lib": ["ESNext"],
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "target": "ESNext",
    "noImplicitAny": false,
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "noEmit": false,
    "declaration": true,
    "emitDeclarationOnly": false,
    "skipLibCheck": true,
    "strict": false,
    "sourceMap": true,
    "outDir": "./dist"
  },
  "include": ["./index.ts"]
}
