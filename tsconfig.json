{
  "compilerOptions": {
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "node",
    "module": "ESNext",
    "target": "ESNext",
    "noImplicitAny": false,
    "allowJs": true,
    "checkJs": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "declaration": false,
    "emitDeclarationOnly": false,
    "types": ["vitest/globals"]
  }
}
